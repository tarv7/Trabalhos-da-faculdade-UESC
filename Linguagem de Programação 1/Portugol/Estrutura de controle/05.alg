
inicio
    Real a, b
    
    Escrever "Digite o valor de A e B: \n"
    Ler a, b
    
    Se (a =/= b)Entao
        Se(a > b)Entao
            Escrever "A diferença entre eles é ", a - b
        Senao
            Escrever "A diferença entre eles é ", b - a
        FimSe
    Senao
        Escrever "A e B são iguais"
    FimSe
fim
