
inicio
    Variavel real n1, n2, res
    
    Escrever "Digite o 1º número: "
    Ler n1
    
    Escrever "Digite o 2º número: "
    Ler n2
    
    res <- n1 + n2
    Escrever n1, " + ", n2, " = ", res, "\n"
    res <- n1 - n2
    Escrever n1, " - ", n2, " = ", res, "\n"
    res <- n1 / n2
    Escrever n1, " / ", n2, " = ", res
fim
